@{
    Layout = null;
}
@model IReadOnlyList<ProductPredictionDto>

@if (Model?.Count > 0)
{
    <ul class="divide-y divide-gray-200 m-0 p-0 z-100">
        @foreach (var item in Model)
        {
            <li>
                <a class="block px-4 py-2 hover:bg-gray-100 text-md"
                   asp-controller="Urun" asp-action="Detaylar"
                   asp-route-id="@item.Id">
                    @item.Name
                </a>
            </li>
        }
    </ul>
}
else
{
    <div class="px-4 py-2 text-gray-500">Sonuç yok</div>
}
