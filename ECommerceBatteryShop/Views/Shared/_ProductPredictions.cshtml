@model IReadOnlyList<string>

@if (Model.Count > 0)
{
    <ul class="divide-y divide-gray-200">
        @foreach (var name in Model)
        {
            <li class="px-4 py-2 hover:bg-gray-100 cursor-pointer">
                @Html.Encode(name)
            </li>
        }
    </ul>
}
else
{
    <div class="px-4 py-2 text-gray-500">No results</div>
}
